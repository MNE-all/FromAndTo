<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FromAndTo" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:windowFullscreen">true</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="backgroundC">@color/white</item>
        <item name="cardViewBgC">@color/dark_blue</item>
        <item name="imgC">@color/blue</item>
        <item name="textC">@color/black</item>
        <item name="textLightC">@color/white</item>
        <item name="menubarC">@color/white</item>
    </style>

    <style name="chipText" parent="TextAppearance.MaterialComponents.Chip">
        <item name="fontFamily">@font/ubuntu_medium</item>
    </style>
    
    <style name="BottomNavigationViewTextStyle">
        <item name="android:fontFamily">@font/ubuntu_medium</item>
    </style>
</resources>